<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sheme4"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="107dp"
        android:layout_height="104dp"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:background="@color/viewfinder_mask"
        android:onClick="StrihActivity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/ic_menu_camera" />

    <Button
        android:id="@+id/button16"
        android:layout_width="242dp"
        android:layout_height="467dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/transparent"
        android:onClick="ListOfModels"
        android:text="ГКШМ"
        android:textAllCaps="false"
        android:textColor="?android:attr/panelBackground"
        app:layout_constraintBottom_toTopOf="@+id/button18"
        app:layout_constraintStart_toEndOf="@+id/button19"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.915" />

    <Button
        android:id="@+id/button17"
        android:layout_width="235dp"
        android:layout_height="212dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="48dp"
        android:background="@android:color/transparent"
        android:onClick="ListOfModels"
        android:text="КАС"
        android:textAllCaps="false"
        android:textColor="?android:attr/panelBackground"
        app:layout_constraintBottom_toTopOf="@+id/button18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/button16" />

    <Button
        android:id="@+id/button24"
        android:layout_width="219dp"
        android:layout_height="214dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="64dp"
        android:background="@android:color/transparent"
        android:onClick="ListOfModels"
        android:text="РССМ"
        android:textAllCaps="false"
        android:textColor="?android:attr/panelBackground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/button16"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button18"
        android:layout_width="332dp"
        android:layout_height="182dp"
        android:layout_marginStart="212dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/transparent"
        android:onClick="ListOfModels"
        android:text="ПостЭлектропитания"
        android:textAllCaps="false"
        android:textColor="?android:attr/panelBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/button20" />

    <Button
        android:id="@+id/button19"
        android:layout_width="184dp"
        android:layout_height="200dp"
        android:layout_marginStart="212dp"
        android:layout_marginTop="132dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/transparent"
        android:onClick="ListOfModels"
        android:orientation="horizontal"
        android:text="ГНРС"
        android:textAllCaps="false"
        android:textColor="?android:attr/panelBackground"
        app:layout_constraintBottom_toTopOf="@+id/button20"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button20"
        android:layout_width="194dp"
        android:layout_height="209dp"
        android:layout_marginStart="204dp"
        android:layout_marginBottom="100dp"
        android:background="@android:color/transparent"
        android:onClick="ListOfModels"
        android:text="ТФСт"
        android:textAllCaps="false"
        android:textColor="?android:attr/panelBackground"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>